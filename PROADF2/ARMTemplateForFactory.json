						"typeProperties": {
							"scripts": [
								{
									"type": "Query",
									"text": {
										"value": "select count(*) as totalcount from @{pipeline().parameters.schema}.@{pipeline().parameters.table}\nwhere last_updated > '@{if(empty(pipeline().parameters.backdate),activity('getdate').output.firstRow.cdc_timestamp,activity('getdate').output.firstRow.cdc_timestamp)}'\n",
